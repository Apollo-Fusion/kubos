schema {
query: Query
mutation: Mutation
}


type Query {
payload: PayloadTelemetry
thruster: ThrusterTelemetry
telemetry(first: Int, subsystem: String, param: String, timestampBefore: Float, timestampAfter: Float): [Telemetry]
}

type Mutation {
payloadEnable(payloadOn: Boolean!): PayloadTelemetry
thrusterEnable(thrusterOn: Boolean!): ThrusterTelemetry
createTelemetry(subsystem: String!, timestamp: Float!, param: String!, value: Float!): Telemetry
}

type PayloadTelemetry {
payloadOn: Boolean
uptime: Float
startTime: Float
}

type ThrusterTelemetry {
thrusterOn: Boolean
}

type Telemetry {
timestamp: Float!
subsystem: String
param: String
value: Float
}
